{
  "name": "performance-render-time",
  "description": "performance",
  "version": "1.1",
  "minimum_chrome_version": "88.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["./contentScript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "devtools_page": "./devtools.html"
}